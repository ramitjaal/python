   "source": [
    "def pascal(n):\n",
    "    if n < 0:\n",
    "        return []\n",
    "    pascal = [[1]*(i+1) for i in range(n)]\n",
    "    for i in range(n):\n",
    "        for j in range(1,i):                \n",
    "            pascal[i][j] = pascal[i-1][j-1] + pascal[i-1][j]\n",
    "    return pascal\n",
    "pascal(5)"
   ]
  }
