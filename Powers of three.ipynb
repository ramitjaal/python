   ],
   "source": [
    "def isPowerOfThree(n=45):\n",
    "    if n == 0:\n",
    "        return False\n",
    "    while (n%3==0):\n",
    "        n/=3\n",
    "\n",
    "    return n==1\n",
    "isPowerOfThree()"
   ]
  }
